{
  "extends": "eslint:recommended",
  "rules": {
    "comma-spacing": "error",
    "curly": ["error", "multi", "consistent"],
    "dot-notation": "error",
    "eol-last": "error",
    "eqeqeq": "error",
    "guard-for-in": "error",
    "indent": ["error", "tab", { "SwitchCase": 1 }],
    "new-parens": "error",
    "no-eval": "error",
    "no-implied-eval": "error",
    "no-restricted-globals": ["error", "fdescribe", "fit"],
    "no-trailing-spaces": "error",
    "no-unused-vars": ["error", { "args": "none", "varsIgnorePattern": "[sShould]" }],
    "semi": ["error", "always"]
  }
}
